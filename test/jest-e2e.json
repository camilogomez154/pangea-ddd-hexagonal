{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/configuration/(.*)": "<rootDir>/../libs/configuration/src/$1",
    "@libs/configuration": "<rootDir>/../libs/configuration/src",
    "@libs/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@libs/database": "<rootDir>/../libs/database/src",
    "@libs/services/(.*)": "<rootDir>/../libs/services/src/$1",
    "@libs/services": "<rootDir>/../libs/services/src"
  }
}